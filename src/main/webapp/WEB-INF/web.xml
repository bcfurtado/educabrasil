<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	id="WebApp_ID" version="2.5">
	<display-name>EducaBrasil</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<description></description>
		<display-name>ListaMunicipios</display-name>
		<servlet-name>ListaMunicipios</servlet-name>
		<servlet-class>org.educabrasil.servlets.ListaMunicipios</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListaMunicipios</servlet-name>
		<url-pattern>/ListaMunicipios</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>sobre</display-name>
		<servlet-name>sobre</servlet-name>
		<servlet-class>org.educabrasil.servlets.Sobre</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>sobre</servlet-name>
		<url-pattern>/sobre</url-pattern>
	</servlet-mapping>
	<servlet>
		<description></description>
		<display-name>informacoes</display-name>
		<servlet-name>informacoes</servlet-name>
		<servlet-class>org.educabrasil.servlets.Informacoes</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>informacoes</servlet-name>
		<url-pattern>/informacoes</url-pattern>
	</servlet-mapping>
	<jsp-config>
		<jsp-property-group>
			<url-pattern>*.jsp</url-pattern>
			<page-encoding>UTF-8</page-encoding>
		</jsp-property-group>
	</jsp-config>
</web-app>